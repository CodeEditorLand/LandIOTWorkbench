{
	"configurations": [
		{
			"name": "Linux",
			"defines": ["ARDUINO=10800"],
			"includePath": [
				"${workspaceFolder}",
				"{ROOTPATH}/.arduino15/packages/esp32/tools/**",
				"{ROOTPATH}/.arduino15/packages/esp32/hardware/esp32/{VERSION}/**"
			],
			"forcedInclude": [
				"{ROOTPATH}/.arduino15/packages/esp32/hardware/esp32/{VERSION}/cores/arduino/Arduino.h"
			],
			"intelliSenseMode": "clang-x64",
			"cStandard": "c11",
			"cppStandard": "c++17",
			"compilerPath": "{ROOTPATH}/.arduino15/packages/esp32/tools/xtensa-esp32-elf-gcc/1.22.0-80-g6c4433a-5.2.0/bin/xtensa-esp32-elf-gcc"
		}
	],
	"version": 4
}
